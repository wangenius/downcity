{
  "name": "downcity",
  "version": "1.0.5",
  "description": "A npm package for downcity agent framework",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "downcity": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "example": "node run-example.js",
    "example:ts": "node --loader ts-node/esm example.ts",
    "example:js": "npm run build && node dist/example.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepublishOnly": "npm run build",
    "publish": "./publish.sh",
    "test": "npm run build && npm i -g . && downcity --help"
  },
  "keywords": [
    "agent framework",
    "downcity",
    "agent",
    "typescript"
  ],
  "author": "wangenius",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangenius/downcity.git"
  },
  "bugs": {
    "url": "https://github.com/wangenius/downcity/issues"
  },
  "homepage": "https://github.com/wangenius/downcity#readme",
  "dependencies": {
    "@ai-sdk/openai": "^2.0.22",
    "ai": "^5.0.24",
    "chalk": "^5.6.0",
    "chromadb": "^1.8.1",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "uuid": "^11.1.0",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.2.21",
    "@types/commander": "^2.12.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
